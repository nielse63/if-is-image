{
  "name": "if-is-image",
  "version": "1.0.1",
  "description": "Check if a string, filepath, or url references an image file",
  "main": "index.js",
  "repository": "https://github.com/nielse63/if-is-image",
  "author": {
    "name": "Erik Nielsen",
    "email": "erik@312development.com",
    "url": "https://312development.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/nielse63/if-is-image",
  "keywords": [
    "ext",
    "extensions",
    "extension",
    "file",
    "path",
    "check",
    "detect",
    "is",
    "image",
    "img",
    "photo",
    "pic",
    "picture",
    "jpg",
    "jpeg",
    "png",
    "gif",
    "webp"
  ],
  "bugs": "https://github.com/nielse63/if-is-image/issues",
  "scripts": {
    "lint": "eslint src ./*.js --ignore-pattern '!.eslintrc.js'",
    "minify": "uglifyjs lib/if-is-image.js -m -c -o lib/if-is-image.min.js",
    "rollup": "rollup --input lib/if-is-image.js --format umd --output lib/if-is-image.js",
    "prebuild": "npm run lint && rm -rf lib && mkdir lib",
    "build": "babel src -d lib",
    "postbuild": "npm run rollup && npm run minify",
    "pretest": "rm -rf coverage *.log",
    "test": "nyc mocha --opts test/unit/mocha.opts test/unit/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublishOnly": "npm test && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-asserttype": "^1.0.5",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rollup": "^0.45.0",
    "uglify-js": "^3.0.24"
  },
  "engines": {
    "node": ">= 8.x",
    "npm": ">= 5.x"
  }
}
